{
  "name": "angular-webpack-seed",
  "version": "0.1.0",
  "description": "angualr project template",
  "main": "index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "setupTestFrameworkScriptFile": "./jest.init.js"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "prod": "npm run process-templates && webpack --env.mode production --progress --colors",
    "build:dev": "process-templates && webpack --env.mode development --progress --colors",
    "dev": "npm run process-templates && webpack-dev-server --env.mode development --progress --colors --hot",
    "lint": "eslint ./src/",
    "test": "./node_modules/.bin/jest",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:update": "./node_modules/.bin/jest -u",
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js",
    "process-templates": "nghtml2js -s es2015 -f 'src/**/*.html' -p src/ -o src/template.js --no-strict",
    "stylelint": "stylelint './src/**/*.scss' --syntax scss"
  },
  "author": "Emiliano Errecalde <eerrecalde@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "angular": "1.4.10",
    "babel-plugin-dynamic-import-webpack": "1.0.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "angular-mocks": "1.4.10",
    "angular-template-cache": "^1.3.1",
    "angular-templatecache-loader": "^0.2.0",
    "autoprefixer": "7.1.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.0.0",
    "babel-loader": "7.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.23.0",
    "codecov.io": "0.1.6",
    "coveralls": "2.13.1",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-config-prettier": "2.1.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "2.1.1",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-loader": "0.4.5",
    "install": "^0.10.4",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.6.4",
    "jest": "^20.0.4",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.0",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-sourcemap-loader": "0.3.5",
    "karma-webpack": "2.0.3",
    "less": "2.7.2",
    "less-loader": "4.0.4",
    "loader-utils": "1.1.0",
    "mini-css-extract-plugin": "^0.4.3",
    "ng-annotate-loader": "0.6.1",
    "ng-url-to-template": "^0.1.5",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "prettier": "1.3.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "0.5.9",
    "webpack": "^4.19.1",
    "webpack-dev-server": "^3.1.8"
  }
}
