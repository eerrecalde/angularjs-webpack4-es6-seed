{
  "extends": [
    "airbnb-base",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "import"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
		"ecmaVersion": 2016,
		"sourceType": "module"
	},
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "import/no-unresolved": 1,
    "global-require": 0,
    "prefer-template": 1
  }
}
